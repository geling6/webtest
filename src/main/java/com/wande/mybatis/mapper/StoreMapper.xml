<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wande.mybatis.dao.StoreMapper">

	<sql id="tableColumns">
        s.store_id, s.no, s.store_name, s.store_pinyin, s.store_initials,
        s.store_address, s.store_phone, s.store_picsrc, s.bg_pic, s.store_floor, s.store_bunk_no,
        s.store_desc, s.plaza_id, s.plaza_name, s.province_id, s.city_id,s.status,
        s.county_id, s.merchant_id, s.merchant_name, s.third_store_id, s.create_time,
        s.update_time, s.version, s.is_valid, s.type, s.business_type,
        s.is_physical_store, s.avg_per_person, s.is_takeout, s.is_order,
        s.takeout_phone, s.support_wifi, s.order_notify_phones, s.store_cate_map_no, s.store_cate_partner_no,
        s.store_english_name, s.store_english_initials, s.store_related_name, s.label, s.ent_group_id, s.ent_group_name,
        s.merchant_source,s.card_no,s.md5,s.creator_id,s.latitude,s.longitude,
        s.store_type,s.store_font_size,s.store_font_brand_id,s.branch_name,s.serivce_tag,s.store_handler,
        s.store_handler_phone,s.business_hours,s.source_id,s.last_update_source_id,s.operation_department_id,s.operator_id
	</sql>
	
	<resultMap type="store" id="storeResultMap">
	  	<id property="storeId" column="store_id"/>
	  	<result property="no" column="no"/>
	  	<result property="storeName" column="store_name"/>
	  	<result property="storePinyin" column="store_pinyin"/>
	  	<result property="storeInitials" column="store_initials"/>
	  	<result property="storeAddress" column="store_address"/>
	  	<result property="storePhone" column="store_phone"/>
	  	<result property="storePicsrc" column="store_picsrc"/>
	  	<result property="bgPic" column="bg_pic"/>
	  	<result property="storeFloor" column="store_floor"/>
	  	<result property="storeBunkNo" column="store_bunk_no"/>
	  	<result property="storeDesc" column="store_desc"/>
	  	<result property="plazaId" column="plaza_id"/>
	  	<result property="plazaName" column="plaza_name"/>
	  	<result property="provinceId" column="province_id"/>
	  	<result property="cityId" column="city_id"/>
	  	<result property="status" column="status"/>
	  	<result property="countyId" column="county_id"/>
	  	<result property="merchantId" column="merchant_id"/>
	  	<result property="merchantName" column="merchant_name"/>
	  	<result property="thirdStoreId" column="third_store_id"/>
	  	<result property="createTime" column="create_time"/>
	  	<result property="updateTime" column="update_time"/>
	  	<result property="version" column="version"/>
	  	<result property="isValid" column="is_valid"/>
	  	<result property="type" column="type"/>
	  	<result property="businessType" column="business_type"/>
	  	<result property="isPhysicalStore" column="is_physical_store"/>
	  	<result property="avgPerPerson" column="avg_per_person"/>
	  	<result property="isTakeout" column="is_takeout"/>
	  	<result property="isOrder" column="is_order"/>
	  	<result property="takeoutPhone" column="takeout_phone"/>
	  	<result property="supportWifi" column="support_wifi"/>
	  	<result property="orderNotifyPhones" column="order_notify_phones"/>
	  	<result property="storeCateMapNo" column="store_cate_map_no"/>
	  	<result property="storeCatePartnerNo" column="store_cate_partner_no"/>
	  	<result property="storeEnglishName" column="store_english_name"/>
	  	<result property="cardNo" column="card_no"/>
	  	<result property="md5" column="md5"/>
	  	<result property="creatorId" column="creator_id"/>
	  	<result property="latitude" column="latitude"/>
	  	<result property="longitude" column="longitude"/>
	  	<result property="storeType" column="store_type"/>
	  	<result property="storeFontSize" column="store_font_size"/>
	  	<result property="storeFontBrandId" column="store_font_brand_id"/>
	  	<result property="branchName" column="branch_name"/>
	  	<result property="serivceTag" column="serivce_tag"/>
	  	<result property="storeHandler" column="store_handler"/>
	  	<result property="storeHandlerPhone" column="store_handler_phone"/>
	  	<result property="businessHours" column="business_hours"/>
	  	<result property="sourceId" column="source_id"/>
	  	<result property="lastUpdateSourceId" column="last_update_source_id"/>
	  	<result property="operationDepartmentId" column="operation_department_id"/>
	  	<result property="operatorId" column="operator_id"/>
	  </resultMap>
	
	<select id="queryStores" resultMap="storeResultMap">
		select 
		<include refid="tableColumns"/>
		from store s
		limit ${offset}, ${limit}
	</select>
	
	<select id="count" resultType="java.lang.Integer">
		select count(*) from store
	</select>
</mapper>